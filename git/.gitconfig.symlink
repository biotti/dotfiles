[core]
    editor = vi
    quotepath = false
[alias]
    graph = log --decorate --oneline --graph
    whatsnew = !git log ..origin/`git rev-parse --abbrev-ref HEAD`
[include]
    path = ~/.gitconfig_user
    path = ~/.gitconfig_include
[init]
    templatedir = ~/.git_template
[pull]
    rebase = false
[fetch]
    prune = false
[rebase]
    autoStash = false

